const ourArray = [];
let i = 0;

while (i < 5) {
  ourArray.push(i);
  i++;
}

console.log(ourArray);

const ourArray = [];

for (let i = 0; i < 5; i++) {
  ourArray.push(i);
}

console.log(ourArray);

